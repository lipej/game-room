{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": true,
            "dynamicImport": true
        },
        "target": "es2020",
        "paths": {
            "#*": [
                "./src/*"
            ],
            "?test/*": [
                "./test/*"
            ]
        },
        "loose": false,
        "externalHelpers": false,
        "baseUrl": "."
    },
    "module": {
        "type": "commonjs"
    },
    "minify": true
}